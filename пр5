#include <stdio.h>

int main() {
    int n;
    printf("Введіть довжину послідовність n: ");
    scanf("%d", &n);
    int mod = 12345;
    int count[n+1];
    count[0] = 1;
    count[1] = 2;
    count[2] = 4;
    for (int i = 3; i <= n; i++) {
        count[i] = (count[i-1] + count[i-2] + count[i-3]) % mod;
    }
    printf("Кількість шуканих послідовностей: %d\n", count[n]);
    return 0;
}
