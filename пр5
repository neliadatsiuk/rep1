#include <stdio.h>

int main() {
    int n;

    // Запитуємо у користувача довжину послідовності
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    int mod = 12345;

    // Оголошуємо масив count для збереження кількості послідовностей
    // Зберігаємо перші три значення
    int count[n+1];
    count[0] = 1;
    count[1] = 2;
    count[2] = 4;

    // Обчислюємо кількість шуканих послідовностей для i > 2
    for (int i = 3; i <= n; i++) {
        // Обчислюємо кількість за формулою, додатково використовуючи модуль mod
        count[i] = (count[i-1] + count[i-2] + count[i-3]) % mod;
    }

    // Виводимо кількість шуканих послідовностей на екран
    printf("Кількість шуканих послідовностей: %d\n", count[n]);

    return 0;
}
