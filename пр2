#include <stdio.h>

// Функція для знаходження найбільшого спільного дільника
int gcd(int a, int b) {
    if (b == 0)
        return a;
    return gcd(b, a % b);
}
// Функція для знаходження найменшого спільного кратного
int lcm(int a, int b) {
    return (a * b) / gcd(a, b);
}
int main() {
    int n; // Кількість чисел
    printf("Введіть кількість чисел: ");
    scanf("%d", &n);
    int numbers[20]; // Масив чисел
    printf("Введіть числа: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &numbers[i]);
    }
    int result = numbers[0]; // Початкове значення результату
    for (int i = 1; i < n; i++) {
        result = lcm(result, numbers[i]);
    }
    printf("Найменше спільне кратне: %d\n", result);

    return 0;
}
