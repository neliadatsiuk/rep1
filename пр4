#include <stdio.h>

int main() {
    int n, m, count = 0;
    
    // Запитуємо у користувача натуральне число n
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);
    
    // Знаходимо кількість рівних дільників числа n
    for (m = 1; m < n; m++) {
        if (n / m == n % m) {
            count++;
        }
    }
    
    // Виводимо кількість рівних дільників на екран
    printf("Кількість рівних дільників числа %d: %d\n", n, count);
    
    return 0;
}
